{
  "name": "entrelibros-frontend",
  "version": "1.0.0",
  "description": "Aplicación web del proyecto EntreLibros: una plataforma colaborativa para compartir, reseñar e intercambiar libros físicos mediante geolocalización.",
  "type": "module",
  "scripts": {
    "dev": "rsbuild dev --open",
    "start": "rsbuild preview",
    "build": "rsbuild build --mode production",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "complete-check": "npm run lint:fix && npm run stylelint:fix && npm run format:fix && npm run typecheck && npm run test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@eslint/js": "^9.32.0",
    "@rsbuild/core": "^1.4.12",
    "@rsbuild/plugin-react": "^1.3.4",
    "@rsbuild/plugin-sass": "^1.3.3",
    "@rsbuild/plugin-svgr": "^1.2.1",
    "@tanstack/react-query": "^5.83.1",
    "@types/react-i18next": "^8.1.0",
    "axios": "^1.11.0",
    "dotenv": "^17.2.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "i18next": "^25.3.2",
    "react": "^19.1.1",
    "react-compiler-runtime": "^19.1.0-rc.2",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.61.1",
    "react-i18next": "^15.6.1",
    "react-router-dom": "^7.7.1",
    "react-toastify": "^11.0.5",
    "stylelint-config-recommended-scss": "^15.0.1",
    "stylelint-config-standard": "^39.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.0.7",
    "css-loader": "^7.1.2",
    "eslint": "^9.32.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jsdom": "^26.1.0",
    "msw": "^2.10.4",
    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "stylelint": "^16.23.0",
    "ts-loader": "^9.5.2",
    "vitest": "^3.0.7"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
