{
  "name": "entrelibros-backend",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": ">=20 <24"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "migrate": "node -r dotenv/config scripts/migrate.js",
    "test": "node scripts/run-tests.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "openapi": "tsx scripts/generate-openapi.ts",
    "complete-check": "npm run lint:fix && npm run format:fix && npm run typecheck && npm run test"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "globals": "^16.4.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.13.1",
    "postgres-migrations": "^5.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "validator": "^13.15.15",
    "socket.io": "^4.7.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.30",
    "@types/pg": "^8.10.3",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/validator": "^13.15.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.32.0",
    "prettier": "^3.6.2",
    "supertest": "^6.3.4",
    "tsx": "^4.7.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vitest": "^3.0.7",
    "socket.io-client": "^4.7.5"
  }
}
